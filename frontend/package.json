{
  "name": "AutoRunLab",
  "private": true,
  "version": "0.1.2",
  "author": "TheOneMind",
  "description": "AutoRunLab",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite --port 51733",
    "build": "vite build",
    "electron:dev": "cross-env NODE_ENV=development concurrently \"vite --port 51733\" \"wait-on tcp:51733 && electron . --verbose\"",
    "electron:build": "vite build && electron-builder",
    "electron:build:publish": "vite build && electron-builder --publish always",
    "electron:build:win": "vite build && electron-builder --config electron-builder.json --win",
    "electron:build:mac": "vite build && electron-builder --config electron-builder.json --mac",
    "preview": "vite preview --port 51733"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.1.7",
    "express": "^4.21.1",
    "lucide-react": "^0.464.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.11.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^33.2.1",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "vite": "^6.0.1",
    "wait-on": "^8.0.1"
  }
}
